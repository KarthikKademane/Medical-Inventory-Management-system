    <body>
        <h1>This is the page which display list of meds</h1>


        <div class="row">
  <div class="col">
    <table class="table table-striped">
    <tr>
    <th scope="col">MID</th>
    <th scope="col">NAME</th>

    <th scope="col">DIAGNOSED TYPE</th>
    <th scope="col">ADMITTED DATE</th>
    </tr>

    <tr>
      {{#rows}}
          <tr>
          <td>{{id}}</td>
          <td>{{name}}</td>

          <td>{{diagnosed_type}}</td>
          <td>{{admitted_date}}</td>

      {{/rows}}
    </tr>
    </table>
  </div>
</div>


    </body>
</html>
